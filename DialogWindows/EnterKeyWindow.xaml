<Window x:Class="CourseProject1.DialogWindows.EnterKeyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CourseProject1.DialogWindows"
        mc:Ignorable="d"
        Title="Изменение ключа" Height="150" Width="300" WindowStyle="ToolWindow" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Введите новый ключ:" HorizontalAlignment="Left" VerticalAlignment="Top"  Width="274" Margin="10,10,0,0"/>
        <Button Content="Готово" Margin="10,0,0,13"  Name="readyButton" IsDefault="True" Click="readyButton_Click" Height="43" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="130"/>
        <Button Content="Отмена" Margin="0,0,7,13" VerticalAlignment="Bottom" Height="43" IsCancel="True" HorizontalAlignment="Right" Width="130"/>
        <TextBox HorizontalAlignment="Left" Margin="10,35,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="267" Name="newKeyTextBox"/>

    </Grid>
</Window>
